import java.util.List;

public class ListSearchIndex{
    public static Integer findLastIndex(List<Integer> list, Integer value) {
        Integer num = 0;
	for (int i = 0 ; i < list.size() ; i++){
		if (list[i].equals(value)){
			num = i ; 
		}
	}
	return num;
    }
